#include <stdio.h>
#include <stdbool.h>

int main() {
    int arr[] = {30, 40, 10, 60, 50, 20, 70, 90, 80, 25};
    int key = 55;
    bool flag = false;
    int i;
    
    for (i = 0; i < 10; i++) {
        if (arr[i] == key) {
            flag = true;
            break;
        }
    }
    
    if (flag == true) {
        printf("Element found\n");
    } else {
        printf("Element not found\n");
    }
    
    return 0;
}