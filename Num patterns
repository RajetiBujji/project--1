#include <stdio.h>

int main() {
    int patternNum;

    
    printf("Enter pattern number (1-14): ");
    scanf("%d", &patternNum);

    switch(patternNum) {
        case 1:
            
            for (int i = 1; i <= 5; i++) {
                printf("%d\n", i);
            }
            break;
        case 2:
            
            for (int i = 1; i <= 5; i++) {
                if (i == 3) {
                    printf("3 3\n");
                } else {
                    printf("%d\n", i);
                }
            }
            break;
        case 3:
            
            for (int i = 1; i <= 5; i++) {
                printf("%d\n", i);
            }
            for (int i = 5; i >= 1; i--) {
                printf("%d\n", i);
            }
            break;
        case 4:
            
            for (int i = 1; i <= 5; i++) {
                for (int j = 1; j <= i; j++) {
                    printf("%d ", i*j);
                }
                printf("\n");
            }
            break;
        case 5:
            // Pattern 5
            for (int i = 1; i <= 5; i++) {
                for (int j = 1; j <= i; j++) {
                    printf("%d ", j*2);
                }
                printf("\n");
            }
            break;
        case 6:
            // Pattern 6
            for (int i = 1; i <= 5; i++) {
                for (int j = 1; j <= i; j++) {
                    printf("%d ", i+j-1);
                }
                printf("\n");
            }
            break;
        case 7:
            // Pattern 7
            for (int i = 1; i <= 5; i++) {
                for (int j = i; j >= 1; j--) {
                    printf("%d ", j);
                }
                printf("\n");
            }
            break;
        case 8:
            // Pattern 8
            for (int i = 1; i <= 5; i++) {
                for (int j = 5; j >= i; j--) {
                    printf("%d ", j);
                }
                printf("\n");
            }
            break;
        case 9:
            
            for (int i = 1; i <= 5; i++) {
                for (int j = 1; j <= i; j++) {
                    printf("%d ", j);
                }
                printf("\n");
            }
            for (int i = 4; i >= 1; i--) {
                for (int j = 1; j <= i; j++) {
                    printf("%d ", j);
                }
                printf("\n");
            }
            break;
        case 10:
            
            for (int i = 1; i <= 5; i++) {
                for (int j = i; j >= 1; j--) {
                    printf("%d ", j);
                }
                printf("\n");
            }
            for (int i = 4; i >= 1; i--) {
                for (int j = i; j >= 1; j