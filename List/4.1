#include <stdio.h>
#include <stdbool.h>

bool isPrime(int n) {
    if (n <= 1)
        return false;
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0)
            return false;
    }
    return true;
}

int main() {
    int start = 11;
    int end = 25;
    int count = 0;

    for (int n = start; n <= end; n++) {
        if (isPrime(n)) {
            printf("%d - Yes\n", n);
            count++;
        } else {
            printf("%d - No\n", n);
        }
    }

    printf("Total count of prime numbers: %d\n", count);

    return 0;
}