#include <stdio.h>

int main() {
    int num = 4375;
    int a = num / 1000;
    int b = (num % 1000) / 100;
    int c = (num / 10) % 10;
    int d = num % 10;

    int res = 100 + b * 10 - a;
    printf("a = %d\n", a);
    printf("b = %d\n", b);
    printf("c = %d\n", c);
    printf("d = %d\n", d);
    printf("res = %d\n", res);

    return 0;
}